{
    "name": "buddy-ui",
    "version": "1.1.0",
    "description": "UI Component Library for personal projects",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "build:esm": "tsc",
        "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
        "build": "rm -rf dist/ && prettier --write src/ && npm run build:esm && npm run build:cjs",
        "webpack": "webpack",
        "webpack-dev-server": "webpack-dev-server",
        "start": "npm run webpack-dev-server -- --env mode=development",
        "prod": "npm run webpack -- --env mode=production"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vinc86/dude-ui.git"
    },
    "keywords": [],
    "author": "Vincenzo Mancuso",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/vinc86/dude-ui/issues"
    },
    "homepage": "https://github.com/vinc86/dude-ui#readme",
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "engines": {
        "node": ">=v14.0.0"
    },
    "dependencies": {
        "styled-components": "^5.3.5"
    },
    "devDependencies": {
        "@babel/cli": "^7.18.10",
        "@babel/core": "^7.18.10",
        "@babel/plugin-transform-runtime": "^7.18.10",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.18.10",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.18.6",
        "@babel/runtime": "^7.18.9",
        "@types/node": "^18.7.1",
        "@types/react": "^18.0.15",
        "@types/react-dom": "^18.0.6",
        "@types/styled-components": "^5.1.26",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.5",
        "babel-plugin-styled-components": "^2.0.7",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
        "css-loader": "^6.7.1",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.5.0",
        "mini-css-extract-plugin": "^2.6.1",
